{
	"applicationId": "com.mcs.front",
	"version": "0.0.1",
	"defaultMode": "window",
	"documentRoot": "/react/build/",
	"url": "/",
	"enableServer": true,
	"enableNativeAPI": true,
	"nativeAllowList": [
		"app.*",
		"filesystem.readDirectory"
	],
	"modes": {
		"chrome": {
			"width": 1280,
			"height": 700,
			"args": "--disable-background-networking --disable-background-timer-throttling --disable-backgrounding-occluded-windows --disable-breakpad --disable-client-side-phishing-detection --disable-default-apps --disable-dev-shm-usage --disable-infobars --disable-extensions --disable-features=site-per-process --disable-hang-monitor --disable-ipc-flooding-protection --disable-popup-blocking --disable-prompt-on-repost --disable-renderer-backgrounding --disable-sync --disable-translate --metrics-recording-only --no-first-run --no-default-browser-check --safebrowsing-disable-auto-update --password-store=basic --use-mock-keychain"
		},
		"window": {
			"title": "Minecraft Creative Studio",
			"width": 1280,
			"height": 700,
			"minWidth": 1280,
			"minHeight": 700,
			"center": true,
			"icon": "/react/public/favicon.ico",
			"enableInspector": true,
			"exitProcessOnClose": true
		}
	},
	"cli": {
		"binaryName": "MCSJv0.0.1",
		"extensionsPath": "/extensions/",
		"resourcesPath": "/react/build/",
		"clientLibrary": "/react/public/__neutralino_globals.js",
		"binaryVersion": "4.10.0",
		"clientVersion": "3.8.2",
		"frontendLibrary": {
			"patchFile": "/react/public/index.html",
			"devUrl": "http://localhost:3000"
		}
	}
}